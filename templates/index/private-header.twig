{% if viewer.permitted('site_torrents_notify') %}
{%     for id, name in viewer.notifyFilters %}
<link rel="alternate" type="application/rss+xml" href="feeds.php?feed=torrents_notify_{{ id }}_{{ viewer.announceKey }}{{ viewer_args }}&amp;name={{ name }}" title="{{ constant('SITE_NAME') }} - {{ name }}" />
{%     endfor %}
{% endif -%}

{{ scss('global.css') }}
