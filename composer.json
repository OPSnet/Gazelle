{
  "name": "orpheusnet/gazelle",
  "description": "Web framework for private BitTorrent trackers using Ocelot",
  "type": "project",
  "license": "Unlicense",
  "authors": [
    { "name": "What.CD" },
    { "name": "Apollo" },
    { "name": "Orpheus" }
  ],
  "autoload": {
    "psr-4": {
      "Gazelle\\": "app/"
    },
    "classmap": ["classes/"]
