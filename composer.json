  "config": {
    "sort-packages": true
  },
  "require": {
    "php": "^8.2",
    "ext-curl": "*",
    "ext-iconv": "*",
    "ext-json": "*",
    "ext-mysqli": "*",
    "ext-openssl": "*",
    "d11wtq/boris": "^v1.0.10",
    "endroid/qr-code": "^5.0",
    "hashids/hashids": "^5.0",
    "maennchen/zipstream-php": "^3.1",
    "monero-integrations/monerophp": "dev-master#25d4c5838b35cbf1fb55170b831e895681a7410a",
    "orpheusnet/bencode-torrent": "^1.3.0",
    "orpheusnet/logchecker": "^0.14.0",
    "protonlabs/bitcoin": "^1.0",
    "robmorgan/phinx": "^0.16",
    "robthree/twofactorauth": "^1.8.2",
    "twig/twig": "^3.14",
    "whichbrowser/parser": "^2.1.8"
  },
  "require-dev": {
    "phpmd/phpmd": "@stable",
    "phpstan/phpstan": "^1",
    "phpunit/phpcov": "^10",
    "phpunit/php-code-coverage": "^11",
    "phpunit/phpunit": "^11",
    "rector/rector": "^1.2",
    "squizlabs/php_codesniffer": "^3.11"
  }
}
